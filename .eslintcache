\r\n    );\r\n}\r\n\r\nexport default ViewUsersPage;","C:\\Revature\\Project1 - WithWard\\withward-frontend\\P1-withward-frontend\\src\\Components\\viewUsers\\userTable.js",[],"C:\\Revature\\Project1 - WithWard\\withward-frontend\\P1-withward-frontend\\src\\Components\\viewUsers\\user.js",[],"C:\\Revature\\Project1 - WithWard\\withward-frontend\\P1-withward-frontend\\src\\Components\\passwordReset.js",["69"],"import React from 'react';\r\nimport { withRouter } from \"react-router-dom\";\r\nimport axios from 'axios';\r\nimport logo from \"../Resources/Images/withward_icon_rainbow.png\";\r\n\r\nclass Register extends React.Component {\r\n\r\n    constructor(props) {\r\n        super(props);\r\n        //If They are not logged in:\r\n        if (!sessionStorage.getItem(\"user\"))\r\n        {\r\n            //Create a new user object to fill\r\n            console.log(\"User not logged in.\")\r\n            this.state = {username:\"\", password:\"\", email:\"\", photo:\"\", errorMessage: null, newPassword:\"\", confirmNewPassword:\"\"};\r\n        }\r\n        else\r\n        {\r\n            //Redirect to their Profile page\r\n            this.state = JSON.parse(sessionStorage.getItem(\"user\"));\r\n        }\r\n    \r\n        this.handleSubmit = this.handleSubmit.bind(this);\r\n        this.handleInputChange = this.handleInputChange.bind(this);\r\n      }\r\n    render() {\r\n      return (\r\n          <div id=\"generic-menu\">\r\n              <form onSubmit={this.handleSubmit}>\r\n                <label>\r\n                <h1><img id='withward-logo' src={logo} alt=\"Withward Logo\"></img>Update Password</h1>\r\n                    Old Password:\r\n                    <br/><input type=\"password\" name=\"password\" value={this.state.password} onChange={this.handleInputChange} />\r\n                    <br/>New Password:\r\n                    <br/><input type=\"password\" name=\"newPassword\" value={this.state.newPassword} onChange={this.handleInputChange} />\r\n                    <br/>Confirm Password:\r\n                    <br/><input type=\"password\" name=\"confirmNewPassword\" value={this.state.confirmNewPassword} onChange={this.handleInputChange} />\r\n                    <br/><input type=\"submit\" value=\"Submit\" className=\"btn\" />\r\n                    <br/><div className=\"error-mesage\">{this.state.errorMessage}</div>\r\n                \r\n                </label>\r\n              </form>\r\n          </div>\r\n          );\r\n    }\r\n    handleSubmit(event) {\r\n        console.log(this.state);\r\n        event.preventDefault();\r\n        if (this.state.newPassword === this.state.confirmNewPassword)\r\n        {\r\n            var urlString = \"http://localhost:8080/withward/users\"\r\n\r\n            //Get the User's ID\r\n            axios.get(urlString, {withCredentials: true})\r\n            .then (resp => \r\n            {\r\n                //Find the User\r\n                let user = resp.data.find(x => x.username == this.state.user.username);\r\n                user.password = this.state.newPassword;\r\n                delete user[\"isAdmin\"];\r\n                //Update User by ID\r\n                console.log(user);\r\n                axios.put(urlString + \"/\" + user.id, user, {withCredentials: true})\r\n                .then (resp => {\r\n                    //Route to User Landing Page\r\n                    alert(\"Successful Changed Password\");\r\n                    // / = Profile Page\r\n                    this.props.history.push(\"/\");\r\n                })\r\n                .catch (resp => {\r\n                    //Registration Failed\r\n                    this.setState({errorMessage: \"Password Update Failed\"})\r\n                });\r\n            })\r\n            .catch (resp =>\r\n            {\r\n                //User Lookup Failed\r\n                console.log(\"Failed to Retrieve User Data from Database\");\r\n                console.log(resp);\r\n            });\r\n\r\n\r\n        }\r\n        else\r\n        {\r\n            this.setState({errorMessage: \"New Passwords Do Not Match\"})\r\n        }\r\n    }\r\n    handleInputChange(event) {\r\n        this.setState({\r\n            [event.target.name]: event.target.value\r\n        });\r\n    }\r\n  }\r\n\r\n\r\n  export default withRouter(Register);","C:\\Revature\\Project1 - WithWard\\withward-frontend\\P1-withward-frontend\\src\\Components\\viewWithlists\\viewWithlistsPage.js",["70","71","72"],"C:\\Revature\\Project1 - WithWard\\withward-frontend\\P1-withward-frontend\\src\\Components\\viewWithlists\\withlistsTable.js",[],"C:\\Revature\\Project1 - WithWard\\withward-frontend\\P1-withward-frontend\\src\\Components\\viewWithlists\\withlist.js",[],{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","severity":1,"message":"78","line":1,"column":8,"nodeType":"79","messageId":"80","endLine":1,"endColumn":12},{"ruleId":"81","severity":1,"message":"82","line":56,"column":8,"nodeType":"83","endLine":56,"endColumn":10,"suggestions":"84"},{"ruleId":"85","severity":1,"message":"86","line":58,"column":59,"nodeType":"87","messageId":"88","endLine":58,"endColumn":61},{"ruleId":"77","severity":1,"message":"89","line":4,"column":10,"nodeType":"79","messageId":"80","endLine":4,"endColumn":20},{"ruleId":"85","severity":1,"message":"86","line":20,"column":55,"nodeType":"87","messageId":"88","endLine":20,"endColumn":57},{"ruleId":"81","severity":1,"message":"90","line":66,"column":8,"nodeType":"83","endLine":66,"endColumn":10,"suggestions":"91"},"no-native-reassign",["92"],"no-negated-in-lhs",["93"],"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'getUsers'. Either include it or remove the dependency array.","ArrayExpression",["94"],"eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected","'useHistory' is defined but never used.","